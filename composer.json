{
  "name": "borschphp/container",
  "description": "A simple PSR-11 Container implementation.",
  "license": "MIT",
  "keywords": ["psr", "psr-11", "container"],
  "authors": [
    {
      "name": "Alexandre DEBUSSCHERE",
      "email": "alexandre.debusschere@hey.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "psr/container": "^2"
  },
  "require-dev": {
      "pestphp/pest": "^1.22",
      "nikic/php-parser": "^4"
  },
  "autoload": {
    "psr-4": {
      "Borsch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BorschTest\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
